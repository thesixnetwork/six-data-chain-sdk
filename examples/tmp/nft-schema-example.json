        "actions": [
          {
            "name": "check_in",
            "desc": "Check In",
            "disable": false,
            "when": "meta.GetBoolean('checked_in') == false",
            "then": [
              "meta.SetBoolean('checked_in', true)",
              "meta.SetNumber('missions_completed', meta.GetNumber('missions_completed') + 1)",
