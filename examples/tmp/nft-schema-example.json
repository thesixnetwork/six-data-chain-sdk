            },
            {
                "name": "redeem_200",
                "desc": "Redeem gift for 200 points",
                "when": "meta.GetBoolean('redeemed') == false && meta.GetNumber('points') >= 200",
                "then": [
                    "meta.SetBoolean('redeemed', true)",
                    "meta.SetNumber('points', meta.GetNumber('points') - 200)",
